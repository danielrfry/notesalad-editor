{
  "name": "notesalad-editor",
  "version": "0.1.1",
  "private": true,
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
